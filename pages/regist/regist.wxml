<view class="contanier">
<view class="toubu">
<image src="../../images/people.png"></image>
</view>
<view class="tb">
      <input placeholder="昵称" />
</view>
</view>
<view class="xm">
      <text decode="{{true}}">姓&ensp;&ensp;&ensp;&ensp;名：</text>
      <input placeholder="请输入姓名" placeholder-style='text-align:right'/>
      <image src="../../images/jiantou.png"></image>
</view>
<view class="xm">
      <text decode="{{true}}">年&ensp;&ensp;&ensp;&ensp;龄：</text>
      <input placeholder="请输入年龄" placeholder-style='text-align:right'  type="number"/>
      <image src="../../images/jiantou.png"></image>
</view>
<view class="xm">
      <text decode="{{true}}">性&ensp;&ensp;&ensp;&ensp;别：</text>
      <input placeholder="男/女" placeholder-style='text-align:right'/>
      <image src="../../images/jiantou.png"></image>
</view>
<view class="xm">
      <text>身份证号：</text>
      <input placeholder="请输入身份证号" placeholder-style='text-align:right' type="idcard"/>
      <image src="../../images/jiantou.png"></image>
</view>
<view class="xm">
      <text decode="{{true}}">学&ensp;&ensp;&ensp;&ensp;校：</text>
      <input  placeholder="请输入大学名称" placeholder-style='text-align:right'/>
      <image src="../../images/jiantou.png"></image>
</view>
<view class="xm">
      <text decode="{{true}}">专&ensp;&ensp;&ensp;&ensp;业：</text>
      <input placeholder="请输入所在专业" placeholder-style='text-align:right'/>
      <image src="../../images/jiantou.png"></image>
</view>
<view class="xm">
      <text decode="{{true}}">学&ensp;&ensp;&ensp;&ensp;号：</text>
      <input placeholder="请输入学号" placeholder-style='text-align:right' />
      <image src="../../images/jiantou.png"></image>
</view>
<view class="xm">
      <text>参与社团：</text>
      <input  placeholder="请输入社团名称" placeholder-style='text-align:right' placeholder-class="input-placeholder"/>
      <image src="../../images/jiantou.png"></image>
</view>
<view class="xm">
      <text>联系电话：</text>
      <input placeholder="请输入联系方式" placeholder-style='text-align:right'  type="number"/>
      <image src="../../images/jiantou.png"></image>
</view>
<view class="btn">点击确认</view>
<view class="user"> 
    <radio-group>
    <radio color="red" checked="false" bindtap="choose" checked="{{check}}"></radio>
    <text>我已阅读并同意</text>
    <text class="red">《用户服务协议》</text>
   </radio-group>

