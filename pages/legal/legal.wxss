page{
  width: 100%;
  height: 100%;
}
.contanier{
  width: 100%;
  height: 100%;
  position: relative;
}
.contanier image{
  width: 100%;
  height: 100%;
  position: absolute;
  opacity: 0.4;
}
.content {
  z-index:1;
  position: absolute;
  padding:40rpx 60rpx;
}
.title{
  text-align: center;
  font-size:32rpx ;
  margin-top: 20rpx;
}
.up{
  font-size: 28rpx;
  color: rgb(31, 30, 30);
  text-align: right;
}
